# Chrome 扩展开发环境配置

NODE_ENV=development

# 构建配置
BUILD_TARGET=extension
BUILD_MODE=development

# 调试配置
DEBUG=true
SOURCE_MAP=true

# 压缩配置（开发环境关闭）
MINIFY=false
DROP_CONSOLE=false
DROP_DEBUGGER=false

# 输出目录
DIST_DIR=dist
PACKAGE_DIR=packages

# 扩展配置
EXTENSION_NAME=Mouse Gesture Extension
EXTENSION_VERSION=1.1.1
EXTENSION_DESCRIPTION=A powerful mouse gesture extension with visual trail effects
